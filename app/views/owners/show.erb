<h1><%=@owner.name%></h1>
<h4>Pets:</h4>
<ul>
  <%@owner.pets.each do |pet|%>
  <li><%=pet.name%></li>
  <%end%>
</ul>

<!--DELETE BUTTON-->
<form action="/owners/<%= @owner.id %>/delete" method="post">
  <input id="hidden" type="hidden" name="_method" value="delete">
  <input type="submit" value="delete">
</form>
